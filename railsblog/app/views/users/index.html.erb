<h3> User Index Page! </h3>
<ul><% @users.each do |user| %>
		<li>
			<%= link_to user.fname+" "+user.lname, "users/#{user.id}" %>

			<%= link_to user.username, "users/#{user.id}" %>

			<%= link_to "edit", edit_user_path(user) %>

			<%= link_to "delete", user_path(user.id), method: :delete %>
		</li>

	<% end %>
</ul>

<%= link_to "Create user", new_user_path %>