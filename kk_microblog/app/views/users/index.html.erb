<h1>User Index Page!</h1>
<ul><% @users.each do |user| %>
		<li>
			<%= link_to user.fname+" "+user.lname, "users/#{user.id}" %>

			<%= link_to user.username, "users/#{user.id}" %>

			<%= link_to "delete", user_path(user), method: :delete, data: {confirm: "Are you sure?"} %>
		</li>

	<% end %>
</ul>

<%= link_to "Create a user", new_user_path %> 